%a)rezolutie temporara de 2ms :
a=1;
f= 0.5;
dc=25;
f=f * 2 * pi;
t=0:.002:5;
y=a * square(f * t,dc);
y(find(y>=0))=0.5; %maxim semnal
y(find(y<0))=-1; %minim semnal
plot(t,y);
axis([0 5 -2 1.5]);xlabel('Time (sec)');ylabel('Amplitude');

%b)rezolutie temporara de 20ms :
a=1;
f= 0.5;
dc=25;
f=f * 2 * pi;
t=0:.02:5;
y=a * square(f * t,dc);
y(find(y>=0))=0.5; %maxim semnal
y(find(y<0))=-1; %minim semnal
plot(t,y);
axis([0 5 -2 1.5]);xlabel('Time (sec)');ylabel('Amplitude');

%c)rezolutie temporara de 200ms :
a=1;
f= 0.5;
dc=25;
f=f * 2 * pi;
t=0:.2:5;
y=a * square(f * t,dc);
y(find(y>=0))=0.5; %maxim semnal
y(find(y<0))=-1; %minim semnal
plot(t,y);
axis([0 5 -2 1.5]);xlabel('Time (sec)');ylabel('Amplitude');
